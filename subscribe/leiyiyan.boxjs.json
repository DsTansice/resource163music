{
  "id": "Leiyiyan.sub",
  "name": "Leiyiyan脚本订阅",
  "author": "@leiyiyan",
  "icon": "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/pyy.png",
  "repo": "",
  "apps": [
    {
      "id": "Code Server",
      "name": "萝卜 Code",
      "author": "@Luobook",
      "repo": "",
      "icons": [
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/pyy.png",
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/pyy.png"
      ],
      "keys": [
        "codeServer"
      ],
      "settings": [
        {
          "id": "@codeServer.open",
          "name": "开启code模式",
          "val": false,
          "type": "boolean",
          "desc": "默认关闭"
        },{
          "id": "@codeServer.address",
          "name": "服务器地址",
          "val": "",
          "type": "textarea",
          "desc": "填入服务器地址，不包括/，如http://127.0.0.1"
        }
      ]
    },
    {
      "id": "Leiyiyan_Netease",
      "name": "网易云音乐签到",
      "author": "@leiyiyan",
      "repo": "",
      "icons": [
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/netease_music.png",
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/netease_music.png"
      ],
      "keys": [
        "Netease_Musician_Cookie",
        "Netease_Musician_UserAgent",
        "Netease_Musician_FansId",
        "Netease_Musician_Enable_Cloud_Shell_Task",
        "Netease_Musician_Enable_Musician_Task",
        "Netease_Musician_Enable_Vip_Task"
      ],
      "settings": [
        {
          "id": "Netease_Musician_Enable_Cloud_Shell_Task",
          "name": "开启云贝任务",
          "val": true,
          "type": "boolean",
          "desc": "默认开启"
        },{
          "id": "Netease_Musician_Enable_Musician_Task",
          "name": "开启音乐人任务",
          "val": true,
          "type": "boolean",
          "desc": "默认开启"
        },{
          "id": "Netease_Musician_Enable_Vip_Task",
          "name": "开启会员任务",
          "val": true,
          "type": "boolean",
          "desc": "默认开启"
        },{
          "id": "Netease_Musician_Cookie",
          "name": "网易云音乐Cookie数据",
          "val": null,
          "type": "textarea",
          "autoGrow": true,
          "desc": "网易云音乐Cookie数据"
        },{
          "id": "Netease_Musician_UserAgent",
          "name": "网易云音乐User-Agent数据",
          "val": null,
          "type": "textarea",
          "autoGrow": true,
          "desc": "网易云音乐User-Agent数据"
        },{
          "id": "Netease_Musician_FansId",
          "name": "网易云音乐粉丝ID数据",
          "val": null,
          "type": "textarea",
          "autoGrow": false,
          "rows": 1,
          "desc": "网易云音乐粉丝ID数据"
        }
      ]
    },
    {
      "id": "Sliverkiss_Yrh",
      "name": "悦然荟小程序",
      "author": "@Sliverkiss",
      "repo": "",
      "icons": [
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/yrh.png",
        "https://raw.githubusercontent.com/leiyiyan/resource/main/icons/yrh.png"
      ],
      "descs_html": [
        "请自行抓包填写悦然荟小程序的 appid、bid、version",
        "填写完成后记得点击此页面底端右下角的“保存”。"
      ],
      "keys": [
        "yrh_data"
      ],
      "settings": [
        {
          "id": "@yrh_data.appid",
          "name": "appid",
          "val": "",
          "type": "text",
          "desc": "填入悦然荟小程序的 appid"
        },{
          "id": "@yrh_data.bid",
          "name": "bid",
          "val": "",
          "type": "text",
          "desc": "填入悦然荟小程序的 bid"
        },{
          "id": "@yrh_data.version",
          "name": "version",
          "val": "",
          "type": "text",
          "desc": "填入悦然荟小程序的 version"
        }
      ]
    }
  ]
}
